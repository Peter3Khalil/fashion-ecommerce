{
  "name": "nextjs-tailwindcss-starter",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "set NODE_ENV=dev && next dev -p 7000",
    "build": "set NODE_ENV=prod && next build",
    "start": "set NODE_ENV=prod && next start -p 7000",
    "lint:fix": "eslint src --fix",
    "lint": "eslint src",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run format && npm run lint:fix && npm run type-check"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.2.1",
    "lucide-react": "^0.428.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  }
}
